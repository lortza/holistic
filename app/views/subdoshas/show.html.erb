<p id="notice"><%= notice %></p>

<h1>
  <strong>Subdosha:</strong>
  <%= display_subdosha_fullname(@subdosha) %>
</h1>

<p>
  <strong>Responsibilty:</strong>
  <%= @subdosha.responsibility %>
</p>

<p>
  <strong>Parent Dosha:</strong>
  <%= link_to @dosha.name, @dosha %>
</p>

<p>
  <strong>Symptoms:</strong>
  <ul>
    <% @symptoms.order("name").each do |symptom| %>
      <li><%= link_to symptom.name, symptom %></li>
    <% end %>
  </ul>
</p>

<%= link_to 'Edit', edit_subdosha_path(@subdosha) %> |
<%= link_to 'Back', subdoshas_path %>
